<?xml version="1.0" encoding="UTF-8"?>
<fetch verson="1.0">
	<property name="res" xpath="//table//tr" fetch="true">
		<property name="ip" xpath="/td[1]/text()" regex="(\d+\.\d+\.\d+\.\d+)"
			require="true" />
		<property name="port" xpath="/td[2]/text()" regex="(\d+)"
			require="true" />
		<property name="address" xpath="/td[5]/text()" />
		<property name="type" value="1"></property>
	</property>
</fetch>